<p id="notice"> </p>
<ul id='task'>
    <h2><%= @task.title %></h2>
  <li>
    <b>Priority:</b>
    <%= @task.priority_string %>
  </li>
  <div class="actions">
    <%= link_to 'Create Task', new_task_subtask_path(@task) %>
    <%= link_to 'Edit', [:edit, @group, @task] %> |
    <%= link_to 'Delete', [@group, @task],
          method: :delete,
          confirm: "Are you sure you want to delete this task?" %> |
    <%= link_to 'Back', @group %>
  </div>
</ul>



