<!-- <p id="notice"><%= notice %></p> -->
<div id='task'>
  <p>
    <b>Title:</b>
    <%= @task.title %>
    <span id='author'>Created by <%= @task.user.email %></span>
  </p>
</div>

<p>
  <b>Description:</b>
  <%= @task.description %>
</p>

<p>
  <b>Priority:</b>
  <%= @task.priority %>
</p>

<p>
  <b>Created at:</b>
  <%= @task.created_at %>
</p>


<%= link_to 'Edit', edit_task_path(@task) %> |
<%= link_to 'Back', tasks_path %>
